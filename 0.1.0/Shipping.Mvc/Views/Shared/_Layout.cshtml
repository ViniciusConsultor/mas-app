<!DOCTYPE html PUBLIC "-//W3C//DTD XHTML 1.0 Strict//EN" "http://www.w3.org/TR/xhtml1/DTD/xhtml1-strict.dtd">
<html xmlns="http://www.w3.org/1999/xhtml" >
<head>
    	<title>
		@if (IsSectionDefined("Title"))
		{
			@RenderSection("Title")
		}
		else
		{
			<text>Untitled</text>
		}
	</title>

    <!-- StyleSheets -->
    <link href="@Url.Content("~/Content/StyleSheets/reset.css")" rel="stylesheet" type="text/css" />
    <link href="@Url.Content("~/Content/StyleSheets/960.css")" rel="stylesheet" type="text/css" />
    <link href="@Url.Content("~/Content/StyleSheets/site.min.css")" rel="stylesheet" type="text/css" />
</head>

<body onunload="">
	<div id="container" class="container_16">
		<div id="header" class="grid_16 alpha omega">
			<div id="logo">
				<img src="@Url.Content("~/Content/Images/logo.png")" alt="VJP" id="logo-img" />
			</div>
		</div>
		<div id="content" class="grid_16 alpha omega">
			@RenderBody()
		</div>
	</div>
	
	<!-- jQuery -->
	<script type="text/javascript" src="https://ajax.googleapis.com/ajax/libs/jquery/1.4.4/jquery.min.js"></script>
	<script type="text/javascript">
		if (!window.jQuery) {
			document.write(unescape("%3Cscript src='@Url.Content("~/Content/Script/jquery-1.4.4.min.js")' type='text/javascript'%3E%3C/script%3E"));
		}
	</script>

	@if (IsSectionDefined("Script"))
	{
		@RenderSection("Script")
	}
</body>
</html>


