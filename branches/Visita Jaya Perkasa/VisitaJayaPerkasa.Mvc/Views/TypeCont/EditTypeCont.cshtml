@model VisitaJayaPerkasa.Mvc.Models.TypeContModel

@{
    ViewBag.Title = "Edit TypeCont";
}

<h2>Edit TypeCont</h2>

<p>
    @Html.ActionLink("Back to List", "Index",
    ViewBag.RouteDicForList as RouteValueDictionary)
</p>

<script src="@Url.Content("~/Scripts/jquery.validate.min.js")" type="text/javascript"></script>
<script src="@Url.Content("~/Scripts/jquery.validate.unobtrusive.min.js")" type="text/javascript"></script>

@Html.ValidationSummary(true, "Edit TypeCont was unsuccessful. Please correct the errors and try again.")

@using (Html.BeginForm())
{
     @Html.HiddenFor(model => model.TypeCode)
    <table>
    <tr>
        <td class="header">@Html.LabelFor(m => m.TypeCode)</td>
        <td class="content">@Html.TextBoxFor(m => m.TypeCode)@Html.ValidationMessageFor(m => m.TypeCode)</td>
    </tr>
    <tr>
        <td class="header">@Html.LabelFor(m => m.TypeName)</td>
        <td class="content">@Html.TextBoxFor(m => m.TypeName)@Html.ValidationMessageFor(m => m.TypeName)</td>
    </tr>
    <tr>
        <td class="header"></td>
        <td class="content"><button name="submit">Edit TypeCont</button></td>
    </tr>
</table>
}
