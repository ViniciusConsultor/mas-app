@using VisitaJayaPerkasa.Web.Mvc
@model VisitaJayaPerkasa.Mvc.Models.SupplierDetailModel
   
    @using (Html.BeginCollectionItem("supplierdetailmodels"))
    {
    <div class="editorRow">
        First Name: @Html.TextBoxFor(x => x.FirstName) 
        Last Name: @Html.TextBoxFor(x => x.LastName)
        Address: @Html.TextBoxFor(x => x.Address) 
        <a href="#" class="deleteRow">delete</a>
        
        @Html.ValidationMessageFor(x => x.FirstName)
        @Html.ValidationMessageFor(x => x.LastName)
        @Html.ValidationMessageFor(x => x.Address)
    </div>   
    }    